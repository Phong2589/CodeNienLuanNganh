<div class="mt-3 mb-3">
    <div class="row">
        {{#each products}}
            <a href="" class="col-xl-3 col-lg-3 col-md-4 col-sm-6 col-12 linkItemProduct">
               <div class="itemProduct">
                <img class="imageProduct w-100" src="{{this.image}}" alt="Lỗi không thể tải ảnh" >
                <div class="">
                    <p>{{this.name}}</p>
                    <p class="number">{{this.cost}}</p>
                    <p>Đã bán: 100</p>
                
                </div>
                </div>
            </a>
        {{/each}}
    </div>
</div>
